<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>English</string>
	<key>CFBundleIdentifier</key>
	<string>org.bitspin.plexapp.eyetv-live</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>1.0</string>
	<!-- 
	Plex specific keys start here
	-->
	<key>PlexClientPlatforms</key>
	<string>MacOSX</string>

	<key>PlexFrameworkVersion</key>
	<string>2</string>
	<!-- 
	We need to use daemon mode or PMS will put us to sleep stopping all streams
	See: http://forums.plexapp.com/index.php/topic/27971-stop-pms-from-sending-plugins-to-sleep/page__view__findpost__p__179779
	-->
	<key>PlexPluginMode</key>
	<string>Daemon</string>
	<!--
	ElevatedPolicy allows for HelperKit 
	-->
	<key>PlexPluginCodePolicy</key>
	<string>Elevated</string>
	<!--
	<key>PlexPluginModuleWhitelist</key>
	<array>
		<string>...</string>
	</array>
	-->
	<!--
	  ## Debug ##
	<key>PlexPluginConsoleLogging</key>
	<string>1</string>
	 ## Debug ##
	-->
	<key>PlexMediaContainer</key>
	<array>
		<string>M4V</string>
		<string>MOV</string>
	</array>
	<key>PlexAudioCodec</key>
	<array>
		<string>AAC</string>
	</array>
	<key>PlexVideoCodec</key>
	<array>
		<string>H.264</string>
	</array>
</dict>
</plist>
